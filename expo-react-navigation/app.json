{
  "name": "expo_react_navigation",
  "displayName": "React Navigation Example",
  "expo": {
    "name": "expo_react_navigation",
    "slug": "expo_react_navigation",
    "description": "Demonstrates the functionality and various capabilities of React Navigation.",
    "privacy": "public",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.howtosave.react_navigation_example"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "org.howtosave.react_navigation_example"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "build": {
        "babel": {
          "include": [
            "victory-native",
            "@babel/plugin-proposal-export-namespace-from"
          ]
        }
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "rne",
    "entryPoint": "App.tsx"
  }
}
